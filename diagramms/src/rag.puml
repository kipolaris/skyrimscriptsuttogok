@startuml rag

title Rag

participant "s : Student" as s
participant "r : Rag" as r
participant "s.location : Room" as room

s->r : ProtectMe("professor")
alt rag.activated == true

r->s : getLocation()
return s.location

r->room : paralyzeProfessors()
return

r-->s : true

else

r-->s : false

end

@enduml